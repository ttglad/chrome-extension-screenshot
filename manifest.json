{
  "name": "screenshots",
  "version": "0.0.1",
  "description": "浏览器截图插件",
  "manifest_version": 3,
  "background": {
    "service_worker": "background.js"
  },
  "permissions": [
    "activeTab",
    "downloads"
  ],
  "icons": {
    "16": "/icons/16.png",
    "32": "/icons/32.png",
    "48": "/icons/48.png",
    "128": "/icons/128.png"
  },
  "host_permissions": [
    "*://*/*"
  ],
  "commands": {
    "screenshots": {
      "suggested_key": {
        "default": "Ctrl+Shift+E"
      },
      "description": "chrome tab screenshots"
    }
  }
}